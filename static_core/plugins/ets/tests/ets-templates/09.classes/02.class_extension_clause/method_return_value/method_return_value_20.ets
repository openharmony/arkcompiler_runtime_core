/*---
desc: The return value of the method is char, and the modifier is public.
name: method_return_value_20
---*/

class Parent {
    public getValue(): char {
        return Int.toChar(100);
    }
}

class Child extends Parent {}

function main(): void {
    let parent = new Parent();
    let parentValue: char = parent.getValue();
    arktest.assertEQ(parentValue, c'd');

    let child = new Child();
    let childValue: char = child.getValue();
    arktest.assertEQ(childValue, c'd');
}
